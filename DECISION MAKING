#Budget
a=float(input())
f=a*350.34 
b=float(input())
g=b*230.90
c=float(input())
h=c*190.55
d=float(input())
i=d*125.30
e=float(input())
j=e*180.90
sum=(f+g+h+i+j)
if sum<15000:
    print("Yes")
else:
    print("No")
    
    
#Sece Dining
a=input()
b=int(input())
if a=="front":
    if b==1:
        print("Left Handed")
    elif b==2:
        print("Right Handed")
    else:
        print("invalid input")
elif a=="rear":
    if b==1:
        print("Right Handed")
    elif b==2:
        print("Left Handed")
    else:
        print("invalid input")
else:
    print("invalid input")

  
#Intersection of a Circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
x=x1-x2
y=y1-y2
c1c2=((x**2)+(y**2))**0.5
if c1c2==r1+r2:
    print("The circles are tangential")
elif c1c2<r1+r2:
     print("The circles overlap")
else:
     print("The circles do not overlap")
     
     
#Microwave Oven
a=int(input("Enter the number of items\n"))
b=float(input("Enter the single item heating time\n"))
if a<2:
    print("The recommended heating time is %f" %b)
elif a==2:
    print("The recommended heating time is %.2f" %(b+50/100*b))
elif a==3:
     print("The recommended heating time is %f" %(2*b))
elif a>3:
        print("Number of items is more")   
