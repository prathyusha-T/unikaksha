#Add an item
a=(4,6,2,8,3,1)
print(a)
b=(4,6,2,8,3,1,9)
print(b)
c=a[0:5]+(15,20,25)+a[0:5]
print(c)
e=list(c)
e.append(30)
d=tuple(e)
print(d)


#Length of a tuple
n=input()
a=tuple(n)
print(a)
c=len(n)
print(c)


#Reverse a Tuple
n=input()
n=tuple(n)
print(n[::-1])


#Repeated element in a tuple
n=input()
print(n)
i=input()
for i in n:
    a=n.count(i)
print(a)
